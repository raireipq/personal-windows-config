    },
    {
      "command": { "action": "moveFocus", "direction": "right" },
      "id": "User.FocusRight"
    },
    {
      "command": { "action": "moveFocus", "direction": "up" },
      "id": "User.FocusUp"
    },
    {
      "command": { "action": "swapPane", "direction": "down" },
      "id": "User.swapDown"
    },
    {
      "command": { "action": "swapPane", "direction": "left" },
      "id": "User.swapLeft"
    },
    {
      "command": { "action": "swapPane", "direction": "right" },
      "id": "User.swapRight"
    },
    {
      "command": { "action": "swapPane", "direction": "up" },
      "id": "User.swapUp"
    }
