  "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
  "initialCols": 100,
  "initialPosition": ",",
  "initialRows": 25,
  "keybindings": [
    {
      "id": "Terminal.CopyToClipboard",
      "keys": "ctrl+c"
    },
    {
      "id": "Terminal.FindText",
      "keys": "ctrl+shift+f"
    },
    {
      "id": "Terminal.PasteFromClipboard",
      "keys": "ctrl+v"
    },
    {
      "id": "User.DuplicatePane",
      "keys": "ctrl+shift+d"
    },
    {
      "id": "User.FocusDown",
      "keys": "ctrl+down"
    },
    {
      "id": "User.FocusLeft",
      "keys": "ctrl+left"
    },
    {
      "id": "User.FocusRight",
      "keys": "ctrl+right"
    },
    {
      "id": "User.FocusUp",
      "keys": "ctrl+up"
    },
    {
      "id": "User.swapDown",
      "keys": "ctrl+shift+down"
    },
    {
      "id": "User.swapLeft",
      "keys": "ctrl+shift+left"
    },
    {
      "id": "User.swapRight",
      "keys": "ctrl+shift+right"
    },
    {
      "id": "User.swapUp",
      "keys": "ctrl+shift+up"
    }
  ],
